{
  "name": "opentxs",
  "version": "1.151.0",
  "description": "The Open-Transactions project is a collaborative effort to develop a robust, commercial-grade, fully-featured, free-software toolkit implementing the OTX protocol as well as a full-strength financial cryptography library, API, CLI, and prototype server. The project is managed by a worldwide community of volunteers that use the Internet to communicate, plan, and develop the Open-Transactions toolkit and its related documentation.",
  "homepage": "https://opentransactions.org",
  "features": {
    "qt5": {
      "description": "Link to Qt5",
      "dependencies": [
        "qt5-base",
        "qt5-declarative"
      ]
    },
    "qt6": {
      "description": "Link to Qt6",
      "dependencies": [
        "qtbase",
        "qtdeclarative"
      ]
    }
  },
  "dependencies": [
    {
      "name": "boost-asio",
      "features": [
        "ssl"
      ]
    },
    "boost-beast",
    "boost-bind",
    "boost-circular-buffer",
    "boost-container",
    "boost-core",
    "boost-dynamic-bitset",
    "boost-endian",
    "boost-exception",
    "boost-interprocess",
    "boost-iostreams",
    "boost-json",
    "boost-lexical-cast",
    "boost-multiprecision",
    "boost-program-options",
    "boost-stacktrace",
    "boost-system",
    "boost-thread",
    "boost-type-index",
    "gtest",
    "libsodium",
    "lmdb",
    "openssl",
    "protobuf",
    "pthread",
    "sqlite3",
    "tbb",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "zeromq",
      "features": [
        "sodium"
      ]
    }
  ]
}
